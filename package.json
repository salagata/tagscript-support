{
  "name": "salagata.tagscript-support",
  "main": "./src/extension.ts",
  "scripts": {
    "test": "vscode-test"
  },
  "icon": "icon.png",
  "displayName": "NotSoBot TagScript Support",
  "description": "NotSoBot TagScript Support based on the web-version, but for VSCode, adding syntax hightlight, code snippets, and more.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nsb",
        "aliases": [
          "NotSoBot TagScript Script",
          "NotSoBot TagScript",
          "nsb"
        ],
        "extensions": [
          ".nsb",
          ".nstb",
          ".mnsb",
          ".mnstb"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nsb",
        "scopeName": "source.nsb",
        "path": "./syntaxes/nsb.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "nsb",
        "path": "./snippets/tagscript.json"
      },
      {
        "language": "nsb",
        "path": "./snippets/advanced_tagscript.json"
      }
    ]
  },
  "dependencies": {
    "vscode": "^1.1.37"
  }
}
