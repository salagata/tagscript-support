{
  "name": "tagscript-coloring",
  "displayName": "TagScript Coloring",
  "description": "NotSoBot TagScript Coloring based on the web-version, but for VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "nsb",
      "aliases": ["NotSoBot TagScript Script", "nsb"],
      "extensions": [".nsb",".nstb",".mnsb",".mnstb"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "nsb",
      "scopeName": "source.nsb",
      "path": "./nsb.tmLanguage.json"
    }]
  }
}
