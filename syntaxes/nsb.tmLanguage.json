{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "NotSoBot TagScript",
  "patterns": [
    {
      "name": "invalid.illegal.nsb",
      "match": "\\{\\{|\\}\\}"
    },
    {
      "include": "#expression"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#languages"
    },
    {
      "include": "#comment"
    }
  ],
  "repository": {
    "expression": {
      "patterns": [
        {
          "include": "#command-names"
        },
        {
          "include": "#tag"
        }
      ]
    },
    "tag": {
      "begin": "\\{",
      "beginCaptures": {
        "0": { "name": "punctuation.brace.curly.open.nsb" }
      },
      "end": "\\}",
      "endCaptures": {
        "0": { "name": "punctuation.brace.curly.close.nsb" }
      },
      "name": "meta.tag.nsb",
      "patterns": [
        {
          "begin": "note",
          "end": "(?=\\})",
          "name": "comment.block.nsb"
        },
        {
          "include": "#tag-symbols"
        },
        {
          "include": "#string"
        },
        {
          "include": "#numbers"
        },
        {
          "include": "#booleans"
        },
        {
          "include": "#expression"
        }
      ]
    },
    "command-names": {
        "name": "keyword.command.nsb",
        "match": "\\b(ignore|note|ai|api\\.create\\.reminder|api\\.search\\.duckduckgo\\.images|api\\.search\\.imgur|api\\.utilities\\.locations|api\\.utilities\\.weather|arg|argsafe|args|argslen|argssafe|attachment|attach|file|last_attachment|lastattachment|lattachment|lattach|attachmentspoiler|attachspoiler|filespoiler|attachmenttext|attachtext|filetext|attachmentvoice|attachvoice|filevoice|avatar|channel|channelid|channelmention|randchannel|randchannelid|randchannelmention|componentjson|componentsontimeout|discord|embedjson|eval|evalsilent|exit|exitsilent|guild|server|guildcount|membercount|servercount|guildid|serverid|sid|gid|hastebin|haste|identify|interrogate|ocr|bracketleft|bracketright|newline|splitterargument|splitterfunction|json\\.channel|channeljson|json\\.guild|json\\.member|json\\.memberoruser|json\\.message|json\\.messagereply|json\\.user|userjson|and|delete|deletechannel|deleteserver|deleteuser|foreach|get|getchannel|getglobal|getserver|getuser|if|iferror|isfromai|isfromcomponent|ismaintag|or|set|setchannel|setglobal|setserver|setuser|math|abs|cos|e|max|min|pi|sin|tan|media|audio|av|image|edit|editurl|imagine|imagineurl|iv|mediascript|mscript|imagescript|iscript|mediascriptmaybeurl|mscriptmaybeurl|imagescriptmaybeurl|iscriptmaybeurl|mediascripturl|mscripturl|imagescripturl|iscripturl|video|messagecontent|messagelastid|randmessageid|messageuserid|nsfw|nsfwfilter|pagejson|prefix|replycontent|replyuserid|request|choose|range|random|rnd|search\\.duckduckgo\\.images|search\\.ddg\\.images|s\\.duckduckgo\\.images|s\\.ddg\\.images|search\\.google\\.images|search\\.g\\.images|s\\.google\\.images|s\\.g\\.images|search\\.youtube|search\\.yt|s\\.youtube|s\\.yt|settings|indexof|jsonify|len|length|lower|markupbold|code|markupcodeblock|markupcodestring|markupescape|markupheaderbig|markupheadermedium|markupheadersmall|markupitalics|markuplistdotted|markuplistnumbered|markupquote|markupspoiler|markupstrike|markupsubtext|markuptime|markupunderline|markupurl|oneof|repeat|replace|replaceregex|reverse|substring|translate|upper|url|urlencode|tag|tagid|tagname|tagownerid|download|text|downloadfromhtml|textfromhtml|unix|unixsnowflake|unixs|transcribe|traversejson|type|useravatar|discrim|id|userid|mention|name|user|nick|randuser|randuserid|randonline|randonlineid|randonlinetag|randusertag|usertag|variables|variableschannel|variablesglobal|variablesserver|variablesuser)\\b"
    },
    "tag-symbols":{
      "patterns": [
        {
          "name": "punctuation.separator.colon.nsb",
          "match": ":"
        },
        {
          "name": "punctuation.separator.pipe.nsb",
          "match": "\\|"
        }
      ]
    },
    "string": {
      "name": "string.quoted.double.nsb",
      "begin": "\"",
      "end": "\"",
      "beginCaptures": { "0": { "name": "punctuation.definition.string.begin.nsb" } },
      "endCaptures": { "0": { "name": "punctuation.definition.string.end.nsb" } },
      "patterns": [
        { "name": "constant.character.escape.nsb", "match": "\\\\." }
      ]
    },
  "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.integer.nsb",
          "match": "\\b\\d+\\b"
        },
        {
          "name": "constant.numeric.float.nsb",
          "match": "\\b\\d+\\.\\d+\\b"
        }
      ]
    },
    "booleans": {
      "patterns": [
        {
          "name": "constant.language.boolean.nsb",
          "match": "\\b(true|false)\\b"
        }
      ]
    }
  },
  "scopeName": "source.nsb"
}